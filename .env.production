
# Production Environment Configuration
NODE_ENV=production
LOG_LEVEL=1

# AI API Keys (set these in Replit Secrets)
GEMINI_API_KEY=
PERPLEXITY_API_KEY=

# TikTok API Configuration (set these in Replit Secrets)
# Get these from https://developers.tiktok.com/
TIKTOK_APP_ID=your_production_app_id
TIKTOK_APP_SECRET=your_production_app_secret
TIKTOK_REDIRECT_URI=https://your-production-domain.repl.co/api/auth/tiktok/callback

# Security (set in Replit Secrets - generate a strong random key)
API_KEY=your_production_api_key

# Application Configuration
TIKTOK_HOURLY_LIMIT=15
TIKTOK_DAILY_LIMIT=200
TIKTOK_WEEKLY_LIMIT=1000
MIN_REQUEST_DELAY_MS=120000
SESSION_TIMEOUT_MS=3600000

# Database
DATABASE_URL=file:./prod.db
